<%- include("links/header"); -%>
        <div class="row">
            <%- include("links/navbar"); -%>
            <div class="col-lg-9 pt-3" style="height:500px;">


                <input class="form-control mb-3" id="myInput" type="text" placeholder="Search..">
                <table class="table table-hover">
                    <thead>
                        <tr class="bg-primary">
                            <th>Phone</th>
                            <th>Amount</th>
                            <th>Details</th>
                            <th>Wallet</th>
                            <th>Transaction date</th>
                        </tr>
                    </thead>
                    <tbody id="myTable">

                        <% for(var i = 0; i< data.length; i++) { %>
                            <tr>
                                <td scope="row"><%= data[i].phone %></td>
                                <td scope="row">
                                    <% if(data[i].type=="CR"){ %>
                                    <label class="text-success">+<%= data[i].amount %></label>
                                    <% }else{ %>
                                        <label class="text-danger">-<%= data[i].amount %></label>
                                    <% } %>

                                </td>
                                <td><%= data[i].details %></td>
                                <td><%= data[i].wallet %></td>
                                <td><%= data[i].created_at %></td>
                            </tr>
                         <% } %>
                        
                    </tbody>
                </table>


            </div>
        </div>
      </div>
      <%- include("links/footer"); -%>